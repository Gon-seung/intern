   "source": [
    "q_movies = df2.copy().loc[df2['vote_count'] >= m]\r\n",
    "q_movies.shape\r\n",
    "# 상위 10퍼의 data_set"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 50,
   "metadata": {},
